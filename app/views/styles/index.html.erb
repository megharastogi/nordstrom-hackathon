<p id="notice"><%= notice %></p>

<h1>Styles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Brandname</th>
      <th>Imageurl</th>
      <th>Styleid</th>
      <th>Shopper</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @styles.each do |style| %>
      <tr>
        <td><%= style.name %></td>
        <td><%= style.brandName %></td>
        <td><%= style.imageURL %></td>
        <td><%= style.styleID %></td>
        <td><%= style.shopper_id %></td>
        <td><%= link_to 'Show', style %></td>
        <td><%= link_to 'Edit', edit_style_path(style) %></td>
        <td><%= link_to 'Destroy', style, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Style', new_style_path %>
